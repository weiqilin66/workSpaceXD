import com.mvc.entity.User;

@Controller
public class ModelController_3 {

	private static final String SUCCESS = "success";
	
	@RequestMapping("testModelAttributes")
	public String testModelAttributes(User user) {
		
		System.out.println(user);
		
		return SUCCESS;
		
	}
}
