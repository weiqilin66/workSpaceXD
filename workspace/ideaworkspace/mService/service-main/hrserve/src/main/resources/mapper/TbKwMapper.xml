<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lwq.hr.mapper.TbKwMapper">
    <insert id="batchInsert">
        insert into tb_kw (name,type) values
        <foreach collection="list" separator="," item="kw">
            (#{kw.name,jdbcType=VARCHAR},#{kw.type,jdbcType=VARCHAR})
        </foreach>
    </insert>
    <delete id="delAll">
        delete from tb_kw
    </delete>

    <select id="selAll" resultType="com.lwq.hr.entity.TbKw">
        select * from tb_kw
    </select>
</mapper>
